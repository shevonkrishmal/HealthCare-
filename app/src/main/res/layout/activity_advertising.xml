<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="429dp"
        android:layout_height="528dp"
        android:translationX="0dp"
        android:translationY="-190dp"
        app:srcCompat="@drawable/finnalogo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-13dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="103dp"
        android:layout_height="49dp"
        android:text="@string/add_post"
        android:background="@color/colorAccent"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="538dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="106dp"
        android:layout_height="48dp"
        android:background="@color/colorAccent"
        android:text="@string/edit_post"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="540dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="118dp"
        android:layout_height="50dp"
        android:text="@string/delete_post"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="538dp"
        android:background="@color/colorAccent"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="210dp"
        android:layout_height="218dp"
        app:srcCompat="@drawable/crm_ad_200cc"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="107dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="174dp"
        android:layout_height="277dp"
        app:srcCompat="@drawable/ms___other_hospitals_thumb"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="85dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="201dp"
        android:layout_height="311dp"
        app:srcCompat="@drawable/covid19_top3worries"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="253dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="168dp"
        android:layout_height="255dp"
        app:srcCompat="@drawable/coronavirus_washing_hands_1"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="285dp" />

</androidx.constraintlayout.widget.ConstraintLayout>